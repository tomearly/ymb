{
  "name": "ymb2",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "deploy" : "ng build &&  cp -r dist/* server/dist/",
    "start": "concurrently \"npm run build-css\" \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "build-css": "node-sass --include-path scss src/styles/main.scss src/styles/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.0-beta.1",
    "@angular/common": "^4.1.0-beta.1",
    "@angular/compiler": "^4.1.0-beta.1",
    "@angular/compiler-cli": "^4.1.0-beta.1",
    "@angular/core": "^4.1.0-beta.1",
    "@angular/forms": "^4.1.0-beta.1",
    "@angular/http": "^4.1.0-beta.1",
    "@angular/platform-browser": "^4.1.0-beta.1",
    "@angular/platform-browser-dynamic": "^4.1.0-beta.1",
    "@angular/platform-server": "^4.1.0-beta.1",
    "@angular/router": "^4.1.0-beta.1",
    "body-parser": "^1.17.1",
    "core-js": "^2.4.1",
    "emailjs": "^1.0.8",
    "express-session": "^1.15.2",
    "method-override": "^2.3.8",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.1",
    "node-restful": "^0.2.5",
    "passwordless": "^1.1.2",
    "passwordless-mongostore": "^0.1.4",
    "pug": "^2.0.0-beta11",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@angular/animations": "^4.1.0-beta.1",
